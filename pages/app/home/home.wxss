@import "../../../components/recommondlist.wxss";
.container{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  margin: 0 0 85rpx 0;
  padding: 0;
  background: #fff
}

.tab-wrapper{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100rpx;
  background: #fff;
}

.tab-left, .tab-right{
  flex: 1;
  font-size: 32rpx;
  color: #000;
  line-height: 100rpx;
  text-align: center;
}

.tab-active{
  border-bottom: 2px solid #09bb07;
}

.content-wrapper{
  width: 100%;
  flex: 1;
}
.searchBar{
  width: 690rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #FAF9F9;
  margin: 39rpx auto 34rpx;
  border-radius: 100rpx;
}
.search{
  height: 100%;
  line-height: 1;
  padding:0 10px;
  font-size: 14px;
  color: #999;
}
.navBar{
  display: flex;
  justify-content: space-around;
  margin-bottom: 40rpx;
  height: 75rpx;
  font-size: 14px;
}