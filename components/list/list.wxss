.list{
  height: 130rpx;
  line-height: 130rpx;
  background: #fff;
  padding: 0 45rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #f1f1f1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list-left text{
  margin-left: 28rpx;
  vertical-align: middle;
}
.list-left .icon{
  width:40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.list-active{
  background: #f9f9f9;
}
.list-right{
  width: 40rpx;
  height: 40rpx;
}